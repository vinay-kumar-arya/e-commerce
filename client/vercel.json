{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json|woff2?|map))",
            "dest": "/$1"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}