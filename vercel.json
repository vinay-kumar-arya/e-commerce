{
    "version": 2,
    "builds": [
        {
            "src": "admin-panel/angular.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "admin-panel/dist/admin-panel"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "admin-panel/dist/admin-panel/index.html"
        }
    ]
}